{
    "name": "deizepi/slim-mvc-raffle",
    "description": "Simple Slim Framework 4 skeleton for raffle based on https://github.com/semhoun/slim-skeleton-mvc project.",
    "license": "MIT",
    "type": "project",
    "keywords": ["slim-framework", "skeleton", "mvc", "sqlite", "twig", "raffle", "monolog", "eloquent", "console"],
    "authors": [
        {
            "name": "Nathanael Semhoun",
            "email": "nathanael@semhoun.net",
            "homepage": "http://www.semhoun.net"
        },
        {
            "name": "David Deizepi",
            "email": "david_deizepi@hotmail.com",
            "homepage": "https://github.com/deizepi"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\Controller\\": "src/Controller",
            "App\\Model\\": "src/Model",
            "App\\Middleware\\" : "src/Middleware",
            "App\\Renderer\\" : "src/Renderer"
        }
    },
    "require": {
        "php": ">=7.1|^8.0",
        "ext-sqlite3": "*",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "php-di/php-di": "^6.3",
        "slim/twig-view": "^3.3",
        "slim/flash": "^0.4.0",
        "monolog/monolog": "^2.0",
        "symfony/console": "^6.0",
        "semhoun/proxy-detection-middleware": "^0.5.0",
        "symfony/cache": "^6.0",
        "robmorgan/phinx": "^0.13.4",
        "respect/validation": "^2.2",
        "illuminate/database": "^10.13",
        "vlucas/phpdotenv": "^5.5"
    }
}
